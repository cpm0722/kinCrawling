<script type="text/javascript" src="//editor-static.pstatic.net/v/kinreply/se.viewer.js?v=1.1.2-20191016143836" defer></script>
<script>
	/**
	 * 에디터 뷰어 새로고침 함수
	 */
	window.refreshSmartEditorViewer = function() {
		if (!window.seViewer) {
			return;
		}

		window.seViewer.refresh();
	};

	/**
	 * 에디터 뷰어 초기화 함수
	 */
	window.initSmartEditorViewer = function() {
		if (typeof SmartEditorViewer === 'undefined' || !SmartEditorViewer) {
			return;
		}

		if (window.seViewer) {
			window.seViewer.destroy();
		}

		window.seViewer = new SmartEditorViewer();

		// 에디터 초기값 설정
		seViewer.bootstrap({
			SERVICE_ID : "kin",
			DEVICE_TYPE : "desktop",
			SERVICE_INAPP : false,
			VIEWER_TYPE : "normal",
			language : "ko-KR",
			RUN_ENV_TYPE : "real",
			MODULE_LOADING_TYPE : "normal",
			MODULE_INJECTION_DATA : {
				VIDEO : {
					ALLOW_SHARE_BUTTON_ALL : {
						hasLink : false
					}
				}
			},
			IMAGE_THUMBNAIL_TYPE : {}
		});

		// 에디터 링크 설정
		seViewer.onLinkResolver({
			"img" : {
				isStop : true,
				callback : function(oData) {
					if (oData.linkUse && oData.link) {
						window.open(oData.link);
					} else if (oData.src) {
						window.open(oData.src);
					}
				}
			},
			"map" : {
				isStop : true,
				callback : function(oData) {
					if (oData.searchEngine == "naver") {
						if (oData.placeId || (oData.latitude && oData.longitude)) {
							window.open("https://map.naver.com/?mapmode=0&pinType=site&title=" + (oData.name || "") + "&pinId=" + (oData.placeId || "") + "&lat=" + (oData.latitude || "") + "&lng=" + (oData.longitude || "") + "&dlevel=10");
						}
					} else {
						if (oData.placeId) {
							window.open("https://www.google.com/maps/search/?api=1&query=" + (oData.name || "") + "&query_place_id=" + (oData.placeId || ""));
						}
					}
				}
			}
		})
	};

	$(document).ready(function () {
		window.initSmartEditorViewer();
	});
</script>

			
			





